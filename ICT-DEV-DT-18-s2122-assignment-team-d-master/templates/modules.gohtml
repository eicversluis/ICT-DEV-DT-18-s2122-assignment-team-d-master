{{template "header.gohtml" .}}
{{template "nav.gohtml" . }}
<h2>Modules</h2>
<table>
    <thead>
    <tr>
        <th scope="col">ID</th>
        <th scope="col">Naam</th>
    </tr>
    </thead>
    <tbody>
    {{ range .}}
        <tr>
            <td>{{.ID}}</td>
            <td><a href="/modulees/{{.ID}}">{{.Name}}</a></td>
        </tr>   
    {{ end }}
    </tbody>
</table>
{{template "footer.gohtml" .}}